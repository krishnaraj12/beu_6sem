<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Information</title>
    <!-- Include Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <h1>Student Information</h1>
        <h2>Registration Number: <%= studentInfo.registration_number %></h2>
        <h2>Student Name: <%= studentInfo.student_name %></h2>

        <!-- Display Theory Subjects Information -->
        <h3>Theory Subjects</h3>
        <table class="table">
            <thead>
                <tr>
                    <th>Subject Code</th>
                    <th>Subject Name</th>
                    <th>ESE</th>
                    <th>IA</th>
                    <th>Total</th>
                    <th>Grade</th>
                    <th>Credit</th>
                </tr>
            </thead>
            <tbody>
                <% studentInfo.theory_subjects_info.forEach(subject => { %>
                    <tr>
                        <td><%= subject.subject_code %></td>
                        <td><%= subject.subject_name %></td>
                        <td><%= subject.ese_marks %></td>
                        <td><%= subject.ia_marks %></td>
                        <td><%= subject.total_marks %></td>
                        <td><%= subject.grade %></td>
                        <td><%= subject.credit %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>

        <!-- Display Practical Subjects Information -->
        <h3>Practical Subjects</h3>
        <table class="table">
            <thead>
                <tr>
                    <th>I</th>
                    <th>II</th>
                    <th>III</th>
                    <th>IV</th>
                    <th>V</th>
                    <th>VI</th>
                    <th>VII</th>
                    <th>VIII</th>
                    <th>Cur. CGPA</th>
                </tr>
            </thead>
            <tbody>
                <% studentInfo.practical_subjects_info.forEach(subject => { %>
                    <tr>
                        <% Object.entries(subject).forEach(([key, value]) => { %>
                            <td><%= value %></td>
                        <% }); %>
                    </tr>
                <% }); %>
            </tbody>
        </table>

        <h3>SGPA: <%= studentInfo.sgpa %></h3>
    </div>
    <!-- Include Bootstrap JS (optional) -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>













